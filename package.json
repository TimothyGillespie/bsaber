{
  "name": "bsaber",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:frontend\" \"npm run netlify-cms\"",
    "dev:frontend": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "create-indices": "node create-indices",
    "netlify-cms": "npx netlify-cms-proxy-server"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@playwright/test": "1.25.0",
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@types/marked": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "sharp": "^0.33.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.7.1",
    "svelte-fa": "^2.4.0",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^4.0.5"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/dm-sans": "^4.5.9",
    "@fontsource/poppins": "^4.5.10",
    "@opensearch-project/opensearch": "^1.0.2",
    "dotenv": "^16.0.0",
    "front-matter": "^4.0.2",
    "js-yaml": "^4.1.0",
    "marked": "^11.2.0",
    "mdsvex": "^0.10.5"
  }
}
